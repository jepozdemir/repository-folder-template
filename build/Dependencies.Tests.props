<Project>
  <Import Project="Dependencies.props" />
  <PropertyGroup>
    <!-- Special case - this property is used by a DotNetCliToolReference -->
    <DotNetXunitVersion>2.4.1</DotNetXunitVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageManagement Include="Microsoft.NET.Test.Sdk" Version="16.6.1" />
    <PackageManagement Include="NSubstitute" Version="4.2.2" />
    <PackageManagement Include="Shouldly" Version="3.0.2" />
    <PackageManagement Include="xunit.analyzers" Version="0.10.0" />
    <PackageManagement Include="xunit" Version="$(DotNetXunitVersion)" />
    <PackageManagement Include="xunit.runner.visualstudio" Version="$(DotNetXunitVersion)" />
    <PackageManagement Include="xunit.extensibility.execution" Version="$(DotNetXunitVersion)" />
    <PackageManagement Include="coverlet.collector" Version="1.2.0" />
  </ItemGroup>
</Project>
